{
  "name": "elysia-backend",
  "version": "1.0.0",
  "prisma": {
    "seed": "bun run prisma/seed"
  },
  "scripts": {
    "start": "bun run dist/app.js",
    "dev": "bun run --watch app.ts",
    "build": "bun build app.ts --outdir dist --target node",
    "prisma:reset": "bunx prisma migrate reset",
    "prisma:migrate": "bunx prisma migrate dev",
    "prisma:generate": "bunx prisma generate"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/static": "^1.3.0",
    "@prisma/client": "^6.9.0",
    "add": "^2.0.6",
    "bcrypt": "^6.0.0",
    "bun": "^1.2.16",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "elysia": "latest",
    "elysia-nnn-router": "^0.0.5",
    "got": "^14.4.7",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "object-key-convert": "^0.0.20",
    "sharp": "^0.34.2",
    "uid": "^2.0.2",
    "zod": "^3.25.62"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^24.0.1",
    "bun-types": "latest",
    "prisma": "^6.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "module": "src/index.js"
}
